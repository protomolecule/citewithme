<h1>Listing #hashtags</h1>

<table id="htags_table" class="table table-bordered table-striped">
  <tr>
    <th>name</th>
    <th>count</th>
  </tr>

<% @tags_with_count.each do |key, value| %>
  <tr>
    <td><b><%= key %></b></td>
    <td><%= value %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to_function "Go back", "history.back()" %>

<script>
startup_htags();
</script>